<?xml version="1.0"?>
<dresscode>
    <resources>
        <resource>
            <title>Megatron Home page</title>
            <identifier>megatron_home</identifier>
            <root_template>one_column</root_template>
            <content><![CDATA[

                <!--Products List-->
                <section class="container content slider-products">
                    {{block type="catalog/product_list" category_id="8"  block_name="Featured Products" template="carousel/products_horizontal_attention.phtml"}}
                </section>
                <!--Brands-->
                <section class="container content-row">
                {{block type="cms/block" block_id="mtron_content_brands" template="cms/content.phtml"}}
                </section>
                <!--Blog widjets-->
                 {{widget type="wordpress/sidebar_widget_posts" title="Recent Posts" number="5" template="wordpress/sidebar/widget/posts.phtml"}}

                <section class="container content slider-products">
                <!-- Filters -->
                  <section class="filters-by-category header-filter clearfix">
                    <ul class="option-set" data-option-key="filter">
                      <li><a href="#filter" data-option-value=".category_id_10" class="selected">Men</a></li>
                      <li>/</li>
                      <li><a href="#filter" data-option-value=".category_id_15">Women</a></li>
                      <li>/</li>
                      <li><a href="#filter" data-option-value=".category_id_8">Unisex</a></li>
                    </ul>
                  </section>
                <!-- //end Filters -->


                <!-- Isotope Products -->
                    <!-- Products list -->
                    <div class="row">
                        <div class="products-list-isotope">
                            {{block type="catalog/product_list" category_id="10" block_name="Men" template="izotope/products_from_category.phtml"}}
                            {{block type="catalog/product_list" category_id="15" block_name="Women" template="izotope/products_from_category.phtml"}}
                            {{block type="catalog/product_list" category_id="8" block_name="Unisex" template="izotope/products_from_category.phtml"}}
                        </div>
                    </div>
                </section>

                <!-- Products widgets -->
                <div class="grey-container-light">
      <section class="container content nopad-bottom hidden-xs">
                    <div class="row  add_after_quick_view_section">
                        <div class="col-sm-6 col-md-3 col-lg-3">
                            {{block type="catalog/product_list" category_id="10"  block_name="Bestsellers" template="carousel/products_vertical.phtml"}}
                        </div>
                        <div class="col-sm-6 col-md-3 col-lg-3">
                            {{block type="catalog/product_new" cache_lifetime="1" products_count="10"  template="carousel/products_new_vertical.phtml"  block_name="New Products"}}
                        </div>
                        <section class="col-sm-6 col-md-3 col-lg-3  slider-products module">
                            {{block type="catalog/product_list" category_id="10"  block_name="On Sale" template="carousel/products_horizontal.phtml"}}
                        </section>
                        <div class="col-sm-6 col-md-3 col-lg-3">
                            {{block type="catalog/product_list" category_id="15"  block_name="Top Rated" template="carousel/products_vertical.phtml"}}
                        </div>
                    </div>
                </section>
                </div>
                <!-- //end Products widget -->



            ]]></content>
            <sort_order>1</sort_order>
            <is_active>1</is_active>
        </resource>


